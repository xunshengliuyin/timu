#include<iostream>
#include<cstdlib>
using namespace std;
int count(int m, int n, int l, int c,int x,int y)
{
	int p=0,r=0;
	if (c == 0)
	{
		if (x == 0)
		{
			p = m + n; cout <<  "ok";

		}

		else  if (x == 1)
		{
			p = m - n;
		}

		else  if (x == 2)
		{
			p = m * n;
		}
		else if (x == 3)
		{
			p = m / n;
		}

		if (y == 0)
		{
			p = p + l;

		}

		else  if (y == 1)
		{
			p = p - l;
		}

		else  if (y == 2)
		{
			p = p * l;
		}
		else if (y == 3)
		{
			p = p / l;
		}
	}
	if (c == 1)
	{


		if (y == 2)
		{
			p = n * l;
		}
		else if (y == 3)
		{
			p = n / l;
		}

		if (x == 0)
		{
			p = p + m;

		}

		else  if (x == 1)
		{
			p = p - m;
		}
		
	}r = p;
	return r;
}


int  main()
{
	int i, a[101][6], b, k, j, l, m, n, s, x, y,c;
	cin >> b;
	for ( i = 0; i < b; i++)
	{
	
		m = (rand() % 100); a[i][0] = m; a[i][0] = m;
		n = (rand() % 100); a[i][1] = n; a[i][1] = n;
		l = (rand() % 100); a[i][2] = l; a[i][2] = l;
			//cout << l;
		x = rand() % 4; a[i][3] = x; 
		y = rand() % 4; a[i][4] = y; 
			if (x == 0)
			{
				cout << m << " + " << n;

			}

			else  if (x==1)
			{
				cout << m << " - " << n;
			}
			
			else  if (x == 2)
			{
				cout << m << " X " << n;
			}
			else if (x == 3)
			{
				cout<<m << "÷" << n;
			}
		
			if (y == 0)
			{
				cout <<  " + " << l;

			}

			else  if (y == 1)
			{
				cout <<  " - " << l;
			}

			else  if (y == 2)
			{
				cout <<  " X " << l;
			}
			else if(y == 3)
			{
				cout << "÷" << l;
			}

			cout << " ="<<endl;

if ((y>=2)&&(x<=1))
	{
		c = 1;
a[i][5] = c;
 
	}
	else
	{
        c = 0;
	}a[i][5] = c;
	}
	
	for ( j = 0; j < b; j++)
	{
		s = count(a[j][0],a[j][1],a[j][2],a[j][5], a[j][3], a[j][4]); 
		cout << "答案是" << s<<endl;
	}
	


	cin >> b;
}
